pipeline{
agent {label 'main'}
    def jsonitem
    stage('AWS CLI Version'){
        sh "aws --version"
    }
    stage('create an ami'){
        def cmd = 'aws ec2 create-image --instance-id i-0298dcecd947ecc05 --name "maheami1" --description "amiserver" --region us-east-2'      
	def output = sh(script: cmd)
        jsonitem = readJSON text: output
        println(jsonitem)
    }
}
